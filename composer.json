{
    "name": "carguru/vendor-bundle",
    "description": "Separate domain logic to bundle",
    "keywords": [
        "symfony",
        "bundle",
        "carguru",
        "vendor",
        "domain",
        "php"
    ],
    "type": "symfony-bundle",
    "homepage": "https://carguru.lv",
    "license": "proprietary",
    "authors": [
        {
            "name": "Jan Rogozinsky",
            "email": "jan.rogozinsky@gmail.com",
            "role": "PHP developer"
        }
    ],
    "require": {
        "php": ">=8",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.14",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/dependency-injection": "^6",
        "symfony/framework-bundle": "^6",
        "symfony/yaml": "6.2.*",
        "vich/uploader-bundle": "^1.22"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpro/grumphp": "^1.3",
        "squizlabs/php_codesniffer": "^3.5",
        "phpstan/phpstan": "^1.7.10"
    },
    "autoload": {
        "psr-4": {
            "Carguru\\VendorBundle\\": "src/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "^6"
        },
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "scripts": {
        "test": "./vendor/bin/grumphp run"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true,
            "composer/package-versions-deprecated": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "prefer-stable": true
}